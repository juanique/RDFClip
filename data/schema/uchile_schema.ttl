@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix clips: <http://www.rdfclip.com/schema#> .
@prefix nfo: <http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#> .
@prefix nie: <http://www.semanticdesktop.org/ontologies/2007/01/19/nie#> .
@prefix uchile: <http://www.rdfclip.com/schema/uchile#> .


#classse
uchile:Curso            rdf:type        rdfs:Class ;
                        rdfs:label      "Curso" ;
                        rdfs:subClassOf rdfs:resource ;
                        clips:userLabel "Curso" .

uchile:MaterialDocente  rdf:type        rdfs:Class ;
                        rdfs:label      "MaterialDocente" ;
                        rdfs:subClassOf rdfs:resource ;
                        clips:userLabel "Material Docente" .

uchile:GradoVisibilidad     rdf:type        rdfs:Class ;
                            rdfs:label      "GradoVisibilidad" ;
                            rdfs:subClassOf rdfs:resource ;
                            clips:userLabel "Grado de visibilidad" .

#properties
uchile:codigoCurso     rdf:type        rdf:Property;
                        rdfs:label      "codigoCurso";
                        rdfs:range      rdfs:Literal;
                        rdfs:domain     uchile:Curso ;
                        clips:userLabel  "Codigo curso" .

uchile:annoCurso        rdf:type        rdf:Property;
                        rdfs:label      "annoCurso";
                        rdfs:range      rdfs:Literal;
                        rdfs:domain     uchile:Curso ;
                        clips:userLabel  "A~no" .

uchile:seccionCurso    rdf:type        rdf:Property;
                        rdfs:label      "seccionCurso";
                        rdfs:range      rdfs:Literal;
                        rdfs:domain     uchile:Curso ;
                        clips:userLabel  "Seccion" .

uchile:tipoCurso       rdf:type        rdf:Property;
                        rdfs:label      "tipoCurso";
                        rdfs:range      rdfs:Literal;
                        rdfs:domain     uchile:Curso ;
                        clips:userLabel  "Tipo" .

uchile:cursoOrigen     rdf:type        rdf:Property;
                        rdfs:label      "cursoOrigen";
                        rdfs:range      rdfs:Curso;
                        rdfs:domain     uchile:MaterialDocente ;
                        clips:userLabel  "Curso de origen" .

